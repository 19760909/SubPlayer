{
    "name": "subplayer",
    "version": "1.0.0",
    "description": "SubPlayer is a online subtitle editor",
    "homepage": "https://subplayer.js.org",
    "dependencies": {
        "artplayer-react": "^3.1.16",
        "dequal": "^1.0.0",
        "duration-time-conversion": "^1.0.0",
        "fetch-jsonp": "^1.1.3",
        "normalize.css": "^8.0.1",
        "nprogress": "^0.2.0",
        "p-limit": "^2.2.1",
        "react": "^16.10.1",
        "react-dom": "^16.10.1",
        "react-i18nify": "^2.0.1",
        "react-scripts": "3.1.2",
        "react-virtualized": "^9.21.1",
        "styled-components": "^4.4.0",
        "toastr": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build && rm -rf docs && mv build docs",
        "lint": "npx eslint src",
        "predeploy": "yarn build",
        "deploy": "gh-pages -d build",
        "sizereport": "npx sizereport --config"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "travis-size-report": "^1.1.0"
    }
}
